require "findsyslibs/version"
require 'net/http'

module Findsyslibs
	repos = Gem.loaded_specs.values
counter = 0
	repos.each do |repo|
  	g_em = repo.name
  	s_ource = repo.source
  	puts g_em
	end

	url = URI.parse('http://localhost:3000')
	req = Net::HTTP::Get.new(url.to_s)
	res = Net::HTTP.start(url.host, url.port) {|http|
	  http.request(req)
	}

	puts res.body

puts 'working'
end
